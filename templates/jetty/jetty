JAVA_HOME=/usr
JAVA=$JAVA_HOME/bin/java
JAVA_OPTIONS=" -server"
JETTY_HOME={{ jetty.home }}
JETTY_STATE={{ jetty.state_directory}}/jetty.state
JETTY_USER=jetty
JETTY_PORT=443
JETTY_HOST=0.0.0.0
JETTY_START_LOG={{ jetty.base }}/logs/jetty-start.log
JETTY_ARGS="jetty.logs={{ jetty.base }}/logs"
JETTY_BASE={{ jetty.base }}
JETTY_START_TIMEOUT=500
JETTY_PID=/var/run/jetty/jetty.pid
